<!--pages/myinfo/materialrecord/materialrecord.wxml-->
<block wx:for="{{suppliesList}}" wx:key="*this">
  <view class="bl-cor">
    <view class="item-ans">
      <view class="avatar">
        <image class="img" src="{{item.tkUserhead}}"></image>
      </view>
      <view class="expertInfo">
        <view class="name">
          <text>任务：</text>{{item.tk_plan_detail_title}}</view>
        <view class="tag">
          <text>备注：</text> {{item.tk_good_apply_remark}}</view>
      </view>
    </view>
    <view class='staffnick'>{{item.tkuserName}}</view>
  </view>
</block>